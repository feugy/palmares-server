{
  "name": "palmares-server",
  "version": "1.0.0",
  "description": "Online server for PalmarÃ¨s web application",
  "main": "lib/server.js",
  "engines": {
    "node": ">=8.4.x"
  },
  "scripts": {
    "lint": "eslint . --fix --ignore-path .gitignore",
    "nsp": "nsp check",
    "test": "eslint . --ignore-path .gitignore && nyc ava --verbose",
    "test:coveralls": "nyc report --reporter=text-lcov | coveralls -v",
    "test:watch": "ava --verbose --watch",
    "start": "node bin/start | pino"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/feugy/palmares.git"
  },
  "author": "Feugy <damien.feugas@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feugy/palmares/issues"
  },
  "homepage": "https://github.com/feugy/palmares#readme",
  "dependencies": {
    "boom": "~6.0.0",
    "cheerio": "~1.0.0-rc.2",
    "csv-parse": "~2.0.0",
    "dotenv": "~4.0.0",
    "hapi": "~16.6.2",
    "hapi-auth-jwt2": "~7.3.0",
    "hapi-pino": "~2.0.0",
    "joi": "~13.0.1",
    "lodash": "~4.17.4",
    "md5": "~2.2.1",
    "moment": "~2.19.1",
    "moment-timezone": "~0.5.14",
    "mongodb": "~2.2.33",
    "pino": "~4.8.1",
    "request": "~2.83.0",
    "request-promise-native": "~1.0.5",
    "sprintf-js": "~1.1.1",
    "underscore.string.fp": "~1.0.4"
  },
  "devDependencies": {
    "ava": "~0.23.0",
    "coveralls": "~3.0.0",
    "eslint": "~4.9.0",
    "eslint-config-standard": "~10.2.1",
    "eslint-plugin-ava": "~4.2.2",
    "eslint-plugin-import": "~2.8.0",
    "eslint-plugin-lodash-fp": "~2.1.3",
    "eslint-plugin-node": "~5.2.0",
    "eslint-plugin-promise": "~3.6.0",
    "eslint-plugin-standard": "~3.0.1",
    "iconv-lite": "~0.4.19",
    "nsp": "~2.8.1",
    "nyc": "~11.2.1"
  },
  "nyc": {
    "include": [
      "lib/**/*"
    ],
    "all": true,
    "check-coverage": true,
    "lines": 95,
    "reporter": [
      "text",
      "html"
    ],
    "temp-directory": "coverage"
  }
}
